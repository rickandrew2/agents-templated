{
  "name": "Express API",
  "description": "RESTful API with Express.js and TypeScript",
  "techStack": {
    "backend": "Node.js (Express)",
    "database": "PostgreSQL or MongoDB",
    "language": "TypeScript"
  },
  "components": ["docs", "rules", "skills", "github"],
  "recommendedPackages": [
    "express",
    "typescript",
    "@types/express",
    "@types/node",
    "dotenv",
    "cors",
    "helmet",
    "express-rate-limit",
    "zod",
    "jsonwebtoken",
    "bcrypt",
    "prisma",
    "jest",
    "@types/jest",
    "supertest",
    "@types/supertest"
  ],
  "gitignore": [
    "# Node",
    "/node_modules",
    "/dist",
    "/build",
    ".env",
    "*.log"
  ],
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "lint": "eslint src/**/*.ts"
  }
}
