{
  "name": "Next.js Full-stack",
  "description": "Next.js with TypeScript, React, and Tailwind CSS",
  "techStack": {
    "frontend": "React / Next.js",
    "backend": "Node.js (Next.js API Routes)",
    "database": "PostgreSQL or Supabase",
    "language": "TypeScript"
  },
  "components": ["docs", "rules", "skills", "github"],
  "recommendedPackages": [
    "next",
    "react",
